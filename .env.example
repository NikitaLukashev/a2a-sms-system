# A2A Protocol Configuration
A2A_PROTOCOL_ID=airbnb-sms-protocol
A2A_AGENT_ID=airbnb-host-agent

# Application Configuration
LOG_LEVEL=INFO
HOST=0.0.0.0
PORT=8000
